{
  "version": 2,
  "builds": [
    {
      "src": "src/index.js",
      "use": "@vercel/node",
      "config": {
        "maxLambdaSize": "50mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/index.js"
    }
  ],
  "env": {
    "AWS_LAMBDA_JS_RUNTIME": "nodejs20.x"
  },
  "functions": {
    "src/index.js": {
      "memory": 1024,
      "maxDuration": 30
    }
  }
}
